{
  "name": "hooyaks",
  "description": "",
  "main": "./lib/index.js",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "commit": "git-cz",
    "exact-semver": "exact-semver",
    "clean": "rimraf lib dist coverage",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/hooyaks.min.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/hooyaks.min.js --config webpack.config.production.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "lint": "eslint test src",
    "test": "NODE_ENV=test mocha",
    "test:watch": "npm test -- --watch",
    "test:cov": "babel-node $(npm bin)/isparta cover $(npm bin)/_mocha",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build && npm run build:umd",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Vasiliy Yorkin <vasiliy.yorkin@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "5.8.21",
    "babel-core": "5.8.22",
    "babel-eslint": "4.0.5",
    "babel-loader": "5.3.2",
    "commitizen": "2.4.6",
    "cz-conventional-changelog": "1.1.4",
    "eslint": "1.1.0",
    "eslint-config-airbnb": "0.1.0",
    "eslint-plugin-react": "3.2.2",
    "exact-semver": "1.2.0",
    "expect": "1.8.0",
    "ghooks": "1.0.1",
    "isparta": "3.0.3",
    "mocha": "2.2.5",
    "rimraf": "2.4.2",
    "semantic-release": "4.3.5",
    "webpack": "1.11.0",
    "webpack-merge": "0.3.0"
  },
  "dependencies": {
    "map-obj": "1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vyorkin/hooyaks.git"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test",
      "update": "npm run exact-semver"
    }
  }
}
